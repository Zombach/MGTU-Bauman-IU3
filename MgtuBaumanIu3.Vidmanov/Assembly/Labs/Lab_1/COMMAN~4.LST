0000: __            |              $ADR   0000H        ; Адрес начала программы

; Занесение исходных данных в регистр
0000: DB 20         |              IN     20H          ; Чтение данных из порта 20h в аккумулятор
0002: 47            |              MOV    B, A         ; Передача содержимого аккумулятора в регистр B

; Проверка на соответствие данных условию 1
0003: E6 04         |              ANI    100B         ; Выполнить логическое умножение содержимого аккумулятора на число 100b
0005: CA 0B 00      |              JZ     ONE          ; Если результат равен нулю,перейти к метке 'ONE'
0008: C3 15 00      |              JMP    END          ; Переход к метке 'END'

; Проверка на соответствие данных условию 2
000B: 78            | ONE:         MOV    A, B         ; Передача содержимого регистра B в аккумулятор
000C: E6 10         |              ANI    10000B       ; Выполнить логическое умножение содержимого аккумулятора на число 10000b
000E: CA 15 00      |              JZ     END          ; Если результат равен нулю,перейти к метке 'END'

; В случае выполнения условий 1 и 2 зажечь светодиоды
0011: 3E FF         |              MVI    A, 0FFH      ; Занесение значения 0FFh в аккумулятор
0013: D3 30         |              OUT    30H          ; Вывод содержимого аккумулятора в порт 30h

0015: 00            | END:         NOP                 ; Метка завершения программы
   -------------------------------------------------
            Обнаруженные идентификаторы

      ONE <000B>              END <0015>
